{
    "env": {
        "node": true, 
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
        "react",
        "react-native",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-use-before-define": 0,
        "react/prop-types": 0,
        "no-console": 0,
        "no-underscore-dangle": [2, { "allowAfterThis": true }]
      }
}